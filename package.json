{
  "name": "boilerplate-local-jwt-auth",
  "version": "0.1.1",
  "description": "Local Passport strategy boilerplate using libsodium and JWTs",
  "main": "server/index.js",
  "scripts": {
    "start": "node server",
    "dev": "nodemon --watch server & webpack --watch",
    "postinstall": "knex migrate:latest && webpack",
    "test": "ava --verbose",
    "test:client": "ava  -v --serial tests/client/**/*.test.js",
    "test:watch": "ava --watch",
    "h:deploy": "git push heroku master",
    "h:migrate": "heroku run knex migrate:latest",
    "h:rollback": "heroku run knex migrate:rollback",
    "h:seed": "heroku run knex seed:run",
    "knex": "knex"
  },
  "author": "EDA",
  "license": "ISC",
  "dependencies": {
    "ava": "^0.17.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.16.0",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "connect-session-knex": "^1.3.1",
    "dotenv": "^4.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.14.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.0.1",
    "express": "^4.14.0",
    "express-force-ssl": "^0.3.2",
    "express-handlebars": "^3.0.0",
    "express-jwt": "^5.1.0",
    "express-session": "^1.15.0",
    "google-maps-react": "^1.1.0",
    "jsdom": "^11.2.0",
    "jsonwebtoken": "^7.4.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.12.6",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^7.2.0",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-places-autocomplete": "^5.4.2",
    "react-redux": "^4.4.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.2.0",
    "sodium": "^2.0.1",
    "sqlite3": "3.1.8",
    "superagent": "^3.5.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.8.0",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-register": "^6.26.0",
    "enzyme": "^2.9.1",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "sqlite3": "^3.1.9",
    "supertest": "^2.0.1",
    "webpack-livereload-plugin": "^0.11.0"
  },
  "ava": {
    "require": [
      "babel-register",
      "dotenv/config"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-3"
    ]
  },
  "engines": {
    "node": ">=7.0.0"
  }
}
